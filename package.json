{
  "name": "difference-hash",
  "version": "1.0.0",
  "description": "Functions to generate the Difference Hash (also known as dhash) of an image",
  "author": "Zaytri",
  "license": "ISC",
  "keywords": [
    "dhash",
    "difference-hash",
    "duplicate",
    "image",
    "detection",
    "similarity"
  ],
  "homepage": "https://github.com/zaytri/difference-hash#readme",
  "bugs": {
    "url": "https://github.com/zaytri/difference-hash/issues"
  },
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "/build"
  ],
  "scripts": {
    "dev": "npm run format && ts-node code/index.ts",
