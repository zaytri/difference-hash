  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaytri/sharp-dhash.git"
  },
  "dependencies": {
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/sharp": "^0.30.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
